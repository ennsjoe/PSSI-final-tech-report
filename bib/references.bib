@article{schoen2017,
  title = {Future of {{Pacific Salmon}} in the {{Face}} of {{Environmental Change}}: {{Lessons}} from {{One}} of the {{World}}'s {{Remaining Productive Salmon Regions}}},
  shorttitle = {Future of {{Pacific Salmon}} in the {{Face}} of {{Environmental Change}}},
  author = {Schoen, Erik R. and Wipfli, Mark S. and Trammell, E. Jamie and Rinella, Daniel J. and Floyd, Angelica L. and Grunblatt, Jess and McCarthy, Molly D. and Meyer, Benjamin E. and Morton, John M. and Powell, James E. and Prakash, Anupma and Reimer, Matthew N. and Stuefer, Svetlana L. and Toniolo, Horacio and Wells, Brett M. and Witmer, Frank D. W.},
