  author = {Olson, Elise M. and Allen, Susan E. and Do, Vy and Dunphy, Michael and Ianson, Debby},
  date = {2020-08},
  journaltitle = {Journal of Geophysical Research: Oceans},
  shortjournal = {JGR Oceans},
  volume = {125},
  number = {8},
  pages = {e2019JC015766},
